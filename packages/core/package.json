{
  "name": "@h3tag-blockchain/core",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc -b",
    "deploy": "AWS_REGIONS=eu-west-1,us-east-1,ap-northeast-1,eu-central-1 DOMAIN_NAME=your-domain.com cdk deploy --all",
    "bootstrap": "AWS_REGIONS=eu-west-1,us-east-1,ap-northeast-1,eu-central-1 ts-node src/network/aws-stack/bootstrap-regions.ts",
    "deploy:all": "yarn bootstrap || exit 1 && yarn deploy",
    "test": "node --max-old-space-size=2048 ./node_modules/.bin/jest",
    "test:watch": "node --max-old-space-size=2048 ./node_modules/.bin/jest --watch --runInBand --config jest.config.ts --detectLeaks --logHeapUsage",
    "test:ci": "node --max-old-space-size=2048 ./node_modules/.bin/jest --runInBand --ci --config jest.config.ts --detectLeaks"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.714.0",
    "@aws-sdk/client-route-53": "^3.714.0",
    "@h3tag-blockchain/shared": "^0.1.0",
    "@h3tag-blockchain/crypto": "^0.1.0",
    "@types/abstract-leveldown": "^7.2.5",
    "@types/node-fetch": "^2.6.12",
    "@types/pg": "^8.11.10",
    "@types/semver": "^7.5.8",
    "abstract-leveldown": "^7.2.0",
    "asn1.js": "^5.4.1",
    "async-mutex": "^0.5.0",
    "aws-cdk": "^2.173.2",
    "aws-cdk-lib": "^2.173.2",
    "bindings": "^1.5.0",
    "bip39": "^3.1.0",
    "bs58": "^6.0.0",
    "@scure/bip32": "^1.3.2",
    "constructs": "^10.4.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "elliptic": "^6.5.4",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "install": "^0.13.0",
    "level": "^9.0.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.13.1",
    "pg-template-tag": "^0.1.2",
    "prom-client": "^15.1.3",
    "ts-node": "^10.9.2",
    "undici": "^7.1.0",
    "winston": "^3.17.0",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@h3tag-blockchain/shared": "0.1.0",
    "@types/bindings": "^1.5.5",
    "@types/bs58": "^4.0.4",
    "@types/commander": "^2.12.5",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.1.1",
    "@types/elliptic": "^6.4.14",
    "@types/express-rate-limit": "^6.0.2",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/pg-template-tag": "^0.1.3",
    "@types/ws": "^8.5.13",
    "@webgpu/types": "^0.1.52",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "node-gyp": "^9.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "license": "MIT",
  "nx": {
    "tags": ["core"],
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": ["{options.outputPath}"],
        "options": {
          "outputPath": "dist",
          "main": "src/index.ts",
          "tsConfig": "tsconfig.json",
          "assets": ["package.json"]
        }
      }
    }
  }
}
